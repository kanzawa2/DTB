<h1>Listing missions</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Deadline</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @missions.each do |mission| %>
      <tr>
        <%= render :partial => "missions/mission_item", :locals => {:mission => mission} %>
      </tr>
    <% end %>
  </tbody>
</table>
