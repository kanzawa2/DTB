<div class="row">
  <div class="span2">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Groups</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>作成データ</td>
        </tr>
        <tr>
          <td>参照データ</td>
        </tr>
        <tr>
          <td>未分類</td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="span6">
    <table class="table table-hover table-responsive scroll">
      <thead>
        <tr>
          <th>Name</th>
          <th>Path</th>
          <th>Duration</th>
        </tr>
      </thead>
      <tbody>
        <% @unified_histories.each do |unified_history| %>
          <%= render :partial => "unified_histories/table_item", :locals => {:unified_history => unified_history} %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
